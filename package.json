{
  "name": "netlify-cms-parcel-example",
  "version": "2.6.0-beta.1",
  "description": "A parcel bundle example to create a NetlifyCMS application.",
  "repository": {
    "type": "git",
    "url": "https://github.com/talves/netlify-cms-parcel-example.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules .cache",
    "start": "run-s clean build:dev",
    "build:dev": "parcel src/index.html",
    "build:parcel": "parcel build src/index.html --out-dir dist --no-cache",
    "build": "run-s clean build:parcel",
    "format": "prettier --write \"src/**/*.js\"",
    "prepublishOnly": "echo \"Create test and lint - (npm run test && npm run lint)\""
  },
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "netlify-cms-backend-bitbucket": "latest",
    "netlify-cms-backend-git-gateway": "latest",
    "netlify-cms-backend-github": "latest",
    "netlify-cms-backend-gitlab": "latest",
    "netlify-cms-backend-test": "latest",
    "netlify-cms-core": "latest",
    "netlify-cms-editor-component-image": "latest",
    "netlify-cms-lib-auth": "latest",
    "netlify-cms-lib-util": "latest",
    "netlify-cms-media-library-cloudinary": "latest",
    "netlify-cms-media-library-uploadcare": "latest",
    "netlify-cms-ui-default": "latest",
    "netlify-cms-widget-boolean": "latest",
    "netlify-cms-widget-date": "latest",
    "netlify-cms-widget-datetime": "latest",
    "netlify-cms-widget-file": "latest",
    "netlify-cms-widget-image": "latest",
    "netlify-cms-widget-list": "latest",
    "netlify-cms-widget-map": "latest",
    "netlify-cms-widget-markdown": "latest",
    "netlify-cms-widget-number": "latest",
    "netlify-cms-widget-object": "latest",
    "netlify-cms-widget-relation": "latest",
    "netlify-cms-widget-select": "latest",
    "netlify-cms-widget-string": "latest",
    "netlify-cms-widget-text": "latest",
    "immutable": "^3.7.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react-immutable-proptypes": "^2.1.0",
    "uuid": "3.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "keywords": [
    "adarta",
    "netlify-cms"
  ],
  "author": "talves",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Opera versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.11.0",
    "parcel-plugin-static-files-copy": "2.0.0",
    "rimraf": "2.6.3"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  }
}
